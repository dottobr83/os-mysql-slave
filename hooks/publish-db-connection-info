#!/bin/bash

mysql_url="mysql://$OPENSHIFT_MYSQLSLAVE_DB_USERNAME:$OPENSHIFT_MYSQLSLAVE_DB_PASSWORD@$OPENSHIFT_GEAR_DNS:$OPENSHIFT_MYSQLSLAVE_DB_PROXY_PORT/"

echo "OPENSHIFT_MYSQLSLAVE_DB_GEAR_UUID=$OPENSHIFT_GEAR_UUID"
echo "OPENSHIFT_MYSQLSLAVE_DB_GEAR_DNS=$OPENSHIFT_GEAR_DNS"
echo "OPENSHIFT_MYSQLSLAVE_DB_USERNAME=$OPENSHIFT_MYSQLSLAVE_DB_USERNAME"
echo "OPENSHIFT_MYSQLSLAVE_DB_PASSWORD=$OPENSHIFT_MYSQLSLAVE_DB_PASSWORD"
echo "OPENSHIFT_MYSQLSLAVE_DB_HOST=$OPENSHIFT_GEAR_DNS"
echo "OPENSHIFT_MYSQLSLAVE_DB_PORT=$OPENSHIFT_MYSQLSLAVE_DB_PROXY_PORT"
echo "OPENSHIFT_MYSQLSLAVE_DB_URL=$mysql_url"
